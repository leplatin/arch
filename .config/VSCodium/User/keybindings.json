// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "alt+cmd+b",
    "command": "workbench.action.toggleActivityBarVisibility"
  },
  {
    "key": "cmd+/",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+cmd+/",
    "command": "editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+alt+cmd+k ctrl+shift+alt+cmd+k",
    "command": "workbench.action.openGlobalKeybindings"
  },
  {
    "key": "ctrl+f1",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "ctrl+shift+alt+cmd+o",
    "command": "extension.liveServer.goOnline",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+=",
    "command": "wwm.aligncode",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.debug.panel.action.clearReplAction"
  },
  {
    "key": "ctrl+shift+alt+cmd+l",
    "command": "extension.debugWrapAfter"
  },
  {
    "key": "ctrl+shift+alt+cmd+w",
    "command": "console.log.wrap",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+down",
    "command": "console.log.wrap.down",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+up",
    "command": "console.log.wrap.up",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+s",
    "command": "console.log.wrap.string",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+numpad_multiply",
    "command": "workbench.action.zoomReset"
  },
  {
    "key": "ctrl+shift+alt+cmd+f",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+alt+cmd+g",
    "command": "editor.action.formatSelection",
    "when": "editorHasDocumentSelectionFormattingProvider && editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+backspace",
    "command": "deleteAllLeft",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "cmd+backspace",
    "command": "editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+/",
    "command": "toggleExplainMode",
    "when": "suggestWidgetVisible"
  },
  {
    "key": "ctrl+shift+alt+cmd+[IntlBackslash]",
    "command": "editor.action.showDefinitionPreviewHover"
  },
  {
    "key": "ctrl+shift+alt+cmd+c",
    "command": "editor.action.insertSnippet",
    "args": {
      "name": "Console Log"
    }
  },
  {
    "key": "ctrl+shift+alt+cmd+a",
    "command": "editor.action.insertSnippet",
    "args": {
      "name": "Arrow Function"
    }
  },
  {
    "key": "alt+cmd+k",
    "command": "workbench.output.action.clearOutput"
  },
  {
    "key": "cmd+k",
    "command": "workbench.action.terminal.clear"
  },
  {
    "key": "cmd+, cmd+.",
    "command": "workbench.action.openSettingsJson"
  },
  {
    "key": "ctrl+shift+alt+cmd+k ctrl+shift+alt+cmd+j",
    "command": "workbench.action.openGlobalKeybindingsFile"
  },
  {
    "key": "cmd+, cmd+,",
    "command": "workbench.action.openSettings2"
  },
  {
    "key": "ctrl+alt+cmd+enter",
    "command": "editor.action.replaceAll",
    "when": "editorFocus && findWidgetVisible"
  },
  {
    "key": "alt+cmd+enter",
    "command": "-editor.action.replaceAll",
    "when": "editorFocus && findWidgetVisible"
  },
  {
    "key": "ctrl+alt+cmd+enter",
    "command": "search.action.replaceAll",
    "when": "replaceActive && searchViewletVisible && !findWidgetVisible"
  },
  {
    "key": "alt+cmd+enter",
    "command": "-search.action.replaceAll",
    "when": "replaceActive && searchViewletVisible && !findWidgetVisible"
  },
  {
    "key": "alt+cmd+enter",
    "command": "editor.action.insertLineBefore",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+enter",
    "command": "-editor.action.insertLineBefore",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+meta+m",
    "command": "workbench.action.toggleMenuBar"
  }
]
